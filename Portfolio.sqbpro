INSERT INTO country_Stock_exchange VALUES (&quot;USA&quot;, &quot;NASDAQ&quot;);</sql><sql name="Views">CREATE VIEW Total_portfolio_value as SELECT sum(NumberOfShares*Last_price) as total_value from Portfolio JOIN Stock_Summary where Portfolio.Tick_Sym = Stock_Summary.Tick_Sym;
CREATE VIEW portfolio_summary as SELECT Portfolio.Tick_Sym, Portfolio.NumberOfShares, Portfolio.Avg_Price, Portfolio.NumberOfShares*Portfolio.Avg_Price as bought_for, 
Portfolio.NumberOfShares*Stock_Summary.Last_price as present_value, (Portfolio.NumberOfShares*Stock_Summary.Last_price - Portfolio.NumberOfShares*Portfolio.Avg_Price) 
as Gain_Loss from Portfolio JOIN Stock_Summary WHERE Portfolio.Tick_Sym = Stock_Summary.Tick_Sym;
CREATE VIEW Canadian_Stock_summary as SELECT * from Stock_Summary where Tick_Sym in 
(SELECT Tick_Sym from Company_Information WHERE Stock_Exchange in (SELECT Stock_exchange from country_Stock_exchange where country = &quot;Canada&quot;));
CREATE VIEW USA_Stock_summary as SELECT * from Stock_Summary where Tick_Sym in 
(SELECT Tick_Sym from Company_Information WHERE Stock_Exchange in (SELECT Stock_exchange from country_Stock_exchange where country = &quot;USA&quot;));
CREATE VIEW tech_stocks as SELECT * from Company_Information WHERE Industry = &quot;Technology&quot;;
CREATE VIEW retail_stocks as SELECT * from Company_Information WHERE Industry = &quot;Retail&quot;;
CREATE VIEW airline_stocks as SELECT * from Company_Information WHERE Industry = &quot;Airlines&quot;;
CREATE VIEW energy_stocks as SELECT * from Company_Information WHERE Industry = &quot;Energy&quot;;
CREATE VIEW entertainment_stocks as SELECT * from Company_Information WHERE Industry = &quot;Entertainment&quot;;
CREATE VIEW gold_stocks as SELECT * from Company_Information WHERE Industry = &quot;Gold&quot;;
CREATE VIEW social_media_stocks as SELECT * from Company_Information WHERE Industry = &quot;Social Media&quot;;
CREATE VIEW Automotive_stocks as SELECT * from Company_Information WHERE Industry = &quot;Automotive&quot;;
CREATE VIEW Stock_percentage_Of_Portfolio as select Portfolio.Tick_Sym, ((Portfolio.NumberOfShares*Stock_Summary.Last_price)*100/
(SELECT sum(Portfolio.NumberOfShares*Stock_Summary.Last_price) from Portfolio JOIN Stock_Summary WHERE Portfolio.Tick_Sym = Stock_Summary.Tick_Sym)) 
as PercentageOfPortfolio from Portfolio JOIN Stock_Summary WHERE Portfolio.Tick_Sym = Stock_Summary.Tick_Sym;
CREATE VIEW Stock_percentage_Of_Portfolio_by_Industry as select Company_Information.Industry, 
(((Portfolio.NumberOfShares*Stock_Summary.Last_price))*100/
(SELECT sum(Portfolio.NumberOfShares*Stock_Summary.Last_price) from Portfolio JOIN Stock_Summary WHERE Portfolio.Tick_Sym = Stock_Summary.Tick_Sym))
 as PercentagePortfolio from Portfolio JOIN Stock_Summary JOIN Company_Information WHERE Portfolio.Tick_Sym = Stock_Summary.Tick_Sym and 
 Stock_Summary.Tick_Sym = Company_Information.Tick_Sym GROUP by Company_Information.Industry;
</sql><sql name="Examples for report">SELECT * from Company_Information WHERE Industry = &quot;Technology&quot;;
